rom local storage\n    useEffect(() => {\n        const storedTodos = JSON.parse(localStorage.getItem(LOCAL_STORAGE_KEY))\n        if (storedTodos)\n            setTodos(storedTodos)\n\n    }, [])\n\n    //save todos to local storage\n    useEffect(() => {\n        localStorage.setItem(LOCAL_STORAGE_KEY, JSON.stringify(todos))\n        //runs this function everytime the todos array change\n    }, [todos])\n\n    function handleAddTodo(e) {\n        //get inputs value\n        const name = todoNameRef.current.value\n        if (name === '') return\n        //setting to dos by adding it to prevTo do array\n        setTodos(prevTodos => {\n            return [...prevTodos, {id: uuidv4(), name: name, complete: false}]\n        })\n        todoNameRef.current.value = null\n    }\n\n    return (\n        //empty tag around all elements bc it can only return one element\n        <>\n            <TodoList todos={todos}/>\n            <input ref={todoNameRef} type=\"text\"/>\n            <button onClick={handleAddTodo}>Add Todo</button>\n            <button>Clear Todos</button>\n            <div>0 left to do</div>\n        </>\n    )\n}\n\nexport default App;\n","/Users/bianca/IdeaProjects/musicalenigma/src/TodoList.js",[],"/Users/bianca/IdeaProjects/musicalenigma/src/Todo.js",[],["28","29"],{"ruleId":"30","replacedBy":"31"},{"ruleId":"32","replacedBy":"33"},{"ruleId":"34","severity":1,"message":"35","line":1,"column":8,"nodeType":"36","messageId":"37","endLine":1,"endColumn":12},{"ruleId":"30","replacedBy":"38"},{"ruleId":"32","replacedBy":"39"},"no-native-reassign",["40"],"no-negated-in-lhs",["41"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar",["40"],["41"],"no-global-assign","no-unsafe-negation"]